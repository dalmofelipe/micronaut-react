{
  "servers": {
    "upstash/context7": {
      "type": "http",
      "url": "https://mcp.context7.com/mcp"
    },
    "mui-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@mui/mcp@latest"
      ]
    },
    "design-systems": {
			"url": "http://localhost:8787/mcp", //https://design-systems-mcp.southleft.com/mcp
			"type": "http"
		},
    "chromedevtools/chrome-devtools-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "chrome-devtools-mcp@latest",
        "--channel=stable",
        "--headless=false",
        "--isolated=false"
      ]
    }
  },
  "inputs": []
}